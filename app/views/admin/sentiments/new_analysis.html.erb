<%= semantic_form_for :analysis, url: create_analysis_admin_sentiments_path, builder: ActiveAdmin::FormBuilder do |f| %>
  <%= f.input :from_datetime, as: :datepicker,
    datepicker_options: { min_date: 1.years.ago.to_date, max_date: Date.today } %>
  <%= f.input :upto_datetime, as: :datepicker,
    datepicker_options: { min_date: 1.years.ago.to_date, max_date: Date.today } %>
  <%= f.actions %>
<% end %>
