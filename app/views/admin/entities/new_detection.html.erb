<%= semantic_form_for :detection, url: create_detection_admin_entities_path, builder: ActiveAdmin::FormBuilder do |f| %>
  <%= f.inputs "New Entities Detection" do %>
    <%= f.input :from_datetime, as: :datepicker,
      datepicker_options: { min_date: 1.years.ago.to_date, max_date: Date.today } %>
    <%= f.input :upto_datetime, as: :datepicker,
      datepicker_options: { min_date: 1.years.ago.to_date, max_date: Date.today } %>
    <% end %>
  <%= f.actions %>
<% end %>
