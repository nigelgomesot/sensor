<%= semantic_form_for :import, url: create_import_admin_messages_path, builder: ActiveAdmin::FormBuilder do |f| %>
  <%= f.inputs "New Slack Import" do %>
    <%= f.input :channel_id %>
    <%= f.input :from_datetime, as: :datepicker,
      datepicker_options: { min_date: 1.years.ago.to_date, max_date: Date.today } %>
    <%= f.input :upto_datetime, as: :datepicker,
      datepicker_options: { min_date: 1.years.ago.to_date, max_date: Date.today } %>
  <% end %>
  <%= f.actions %>
<% end %>
